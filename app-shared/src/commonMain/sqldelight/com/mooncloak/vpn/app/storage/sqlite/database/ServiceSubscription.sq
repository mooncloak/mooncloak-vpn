import kotlinx.datetime.Instant;

CREATE TABLE ServiceSubscription (
    databaseId INTEGER PRIMARY KEY NOT NULL,
    id TEXT NOT NULL,
    planId TEXT NOT NULL,
    created INTEGER AS Instant NOT NULL,
    boosted INTEGER AS Instant,
    expiration INTEGER AS Instant NOT NULL,
    boosts INTEGER,
    bytes INTEGER
);

CREATE INDEX ServiceSubscription_id_index ON ServiceSubscription(id);
CREATE INDEX ServiceSubscription_plan_id_index ON ServiceSubscription(planId);
